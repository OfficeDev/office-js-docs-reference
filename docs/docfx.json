{
  "build": {
    "content": [
      {
        "files": [

          "**/toc.md",
          "**/toc.yml"
        ],
        "src" : "docs-ref-autogen",
        "dest": "api/office_ref_toc",
        "version": "office-js"
      },
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "src": "docs-ref-conceptual",
        "dest": "office",
        "version": "office-js",
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "**/_themes/**",
          "**/_themes.pdf/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ]
      },
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "src": "docs-ref-autogen",
        "dest": "api",
        "version": "office-js",
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "**/_themes/**",
          "**/_themes.pdf/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ]
      },
      {
        "files": ["breadcrumb/toc.yml"]
      },
      {
        "files": ["**/**.md",
                  "**/**.yml"],
        "exclude": ["**/obj/**"]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**"

        ],
        "dest": "api",
        "version": "office-js"
      },
      {
        "files": [
          "**/*.png",
          "**/*.jpg",
          "**/*.svg"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ],
        "src": "docs-ref-conceptual",
        "version": "office-js",
        "dest": "office"
      }
    ],
    "versions": {
      "office-js": {
        "dest": "office-js"
      }
    },
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "breadcrumb_path": "/javascript/office/breadcrumb/toc.json",
       "extendBreadcrumb": true,
      "ms.author": "o365devx",
      "apiPlatform": "javascript",
      "ms.date": "08/15/2018",
      "author": "o365devx",
      "ms.prod": "office 365",
      "ms.technology": "add-ins",
      "ms.topic": "reference",
      "ms.devlang": "JavaScript",
      "product": ["office 365"],
      "feedback_system": "GitHub",
      "feedback_github_repo": "OfficeDev/office-js-docs-reference",
      "feedback_product_url": "https://officespdev.uservoice.com/" 
    },
    "fileMetadata": {
      "monikers": {
        "docs-ref-autogen/**": [
          "office-js"
        ]
      }
    },
    "template": [],
    "dest": "office-docs-ref-javascript"
  }
}

