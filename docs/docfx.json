{
  "globalMetadata": {
    "feedback_system": "GitHub",
    "feedback_github_repo": "MicrosoftDocs/visualstudio-docs",
    "feedback_product_url": "https://developercommunity.visualstudio.com/"     
  },
  "build": {
    "content": [
      {
        "files": [
          "**/toc.md",
          "**/toc.yml"
        ],
        "src" : "docs-ref-autogen",
        "dest": "api/office_ref_toc"
      },
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "src": "docs-ref-conceptual",
        "dest": "office",
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ]
      },
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "src": "docs-ref-autogen",
        "dest": "api",
        "exclude": [
          "**/*..yml",
          "*/toc.yml"
        ]
      },
      {
        "files": [
          "**/*.yml"
        ],
        "src": "breadcrumb",
        "dest": "office/breadcrumb",
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ],
        "dest": "api"
      },
      {
        "files": [
          "**/*.png",
          "**/*.jpg",
          "**/*.svg"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ],
        "src": "docs-ref-conceptual",
        "dest": "office"
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "breadcrumb_path": "/javascript/office/breadcrumb/toc.json"
    },
    "fileMetadata": {},
    "template": [],
    "dest": "office-docs-ref-javascript"
  }
}
