{
  "$schema": "https://dev.office.com/json-schemas/api-extractor/api-extractor.schema.json",
  "compiler" : {
    "configType": "tsconfig",
    "rootFolder": "."
  },
  "policies": {
    "namespaceSupport": "permissive"
  },
  "validationRules": {
    "missingReleaseTags": "allow"
  },
  "project": {
    "entryPointSourceFile": "outlook.d.ts"
  },
  "apiReviewFile": {
    "enabled": false
  },
  "apiJsonFile": {
    "enabled": true,
    "outputFolder": "../json"
  }
}
